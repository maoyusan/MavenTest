<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <!--初始化参数-->
    <context-param>
        <param-name>url</param-name>
        <param-value>jdbc:mysql://localhost:3306</param-value>
    </context-param>
    <!--注册servlet-->
    <servlet>
        <servlet-name>sd3</servlet-name>
        <servlet-class>ServletDemo03</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>sd4</servlet-name>
        <servlet-class>ServletDemo04</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>sd5</servlet-name>
        <servlet-class>ServletDemo05</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>default</servlet-name>
        <servlet-class>HelloServlet</servlet-class>

    </servlet>
    <servlet>
        <servlet-name>404</servlet-name>
        <servlet-class>Error</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>getC</servlet-name>
        <servlet-class>GetServlet</servlet-class>
    </servlet>
    <!--请求响应路径-->
    <servlet-mapping>
        <servlet-name>sd3</servlet-name>
        <url-pattern>/sd3</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>sd5</servlet-name>
        <url-pattern>/sd5</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>sd4</servlet-name>
        <url-pattern>/sd4</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>getC</servlet-name>
        <url-pattern>/get</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>default</servlet-name>
        <url-pattern>/index</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>404</servlet-name>
        <url-pattern>/*</url-pattern>
    </servlet-mapping>
</web-app>